{
  "name": "Kalman courses-ros2-concepts-l1 DevContainer",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "courses-ros2-concepts-l1",
  "workspaceFolder": "/kalman_ws",
  "remoteUser": "kalman",

  "shutdownAction": "stopCompose",
  "initializeCommand": "bash .devcontainer/init.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools-extension-pack",
        "ranch-hand-robotics.rde-pack",
        "smilerobotics.urdf",
        "morningfrog.urdf-visualizer",
        "ms-vscode.cmake-tools"
      ]
    }
  }
}
